{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "right",
      "newline": true,
      "segments": [
        {
          "foreground": "#ff8080",
          "properties": {
            "template": "executed in {{ .FormattedMs }} <#ffffff,>|</> ",
            "threshold": 500
          },
          "style": "plain",
          "type": "executiontime"
        },
        {
          "foreground": "#007acc",
          "properties": {
            "template": "{{ .Name }}"
          },
          "style": "plain",
          "type": "shell"
        },
        {
          "foreground": "#ff8080",
          "properties": {
            "root_icon": "root",
            "template": " <#ffffff,>as</> \uf0e7"
          },
          "style": "plain",
          "type": "root"
        },
        {
          "foreground": "#91ddff",
          "properties": {
            "template": " <#ffffff,>|</> {{ .CurrentDate | date .Format }}",
            "time_format": "15:04:05"
          },
          "style": "plain",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#91ddff",
          "properties": {
            "display_host": true,
            "template": "\u250c {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
          },
          "style": "plain",
          "type": "os"
        },
        {
          "foreground": "#ffffff",
          "properties": {
            "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }}@{{ .HostName }} "
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "#91ddff",
          "properties": {
            "style": "mixed",
            "template": " {{ .Path }} "
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "#95ffa4",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f73535{{ end }}",
            "{{ if gt .Ahead 0 }}#ffee75{{ end }}",
            "{{ if gt .Behind 0 }}#91ddff{{ end }}"
          ],
          "properties": {
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone": "\u2262",
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u2261",
            "cherry_pick_icon": "\u2713 ",
            "commit_icon": "\u25b7 ",
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 ",
            "template": " {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} \u2502{{ end }}{{ if .Staging.Changed }}{{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}}{{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} "
          },
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "#178600",
          "properties": {
            "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} "
          },
          "style": "plain",
          "type": "dotnet"
        },
        {
          "foreground": "#B07219",
          "properties": {
            "template": " \ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
          },
          "style": "plain",
          "type": "java"
        },
        {
          "foreground": "#73AA63",
          "properties": {
            "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} "
          },
          "style": "plain",
          "type": "node"
        },
        {
          "foreground": "#CD2A18",
          "properties": {
            "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
          },
          "style": "plain",
          "type": "ruby"
        },
        {
          "foreground": "#ff8080",
          "properties": {
            "template": " {{ if gt .Code 0 }}error{{ else }}\uf42e{{ end }} "
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#91ddff",
          "properties": {
            "template": "\u2514"
          },
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#ffffff",
          "properties": {
            "template": "$"
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "osc99": true,
  "version": 1
}
