{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "osc99": true,
    "blocks": [
      {
        "type": "prompt",
        "newline": true,
        "alignment": "right",
        "segments": [
          {
            "type": "executiontime",
            "style": "plain",
            "foreground": "#ff8080",
            "properties": {
              "threshold": 500,
              "prefix": "executed in ",
              "postfix": " <#ffffff,>|</> "
            }
          },
          {
            "type": "shell",
            "style": "plain",
            "foreground": "#007acc",
            "properties": {
              "prefix": "",
              "postfix": ""
            }
          },
          {
            "type": "root",
            "style": "plain",
            "foreground": "#ff8080",
            "properties": {
              "prefix": " <#ffffff,>as</> ",
              "postfix": "",
              "root_icon": "root"
            }
          },
          {
            "type": "time",
            "style": "plain",
            "foreground": "#91ddff",
            "properties": {
              "prefix": " <#ffffff,>|</> ",
              "postfix": "",
              "time_format": "15:04:05"
            }
          }
        ]
      },
      {
        "type": "prompt",
        "newline": true,
        "alignment": "left",
        "segments": [
          {
            "type": "os",
            "style": "plain",
            "foreground": "#91ddff",
            "properties": {
              "prefix": "\u250c ",
              "display_host": true
            }
          },
          {
            "type": "session",
            "style": "plain",
            "foreground": "#ffffff"
          },
          {
            "type": "path",
            "style": "plain",
            "foreground": "#91ddff",
            "properties": {
              "style": "mixed",
              "enable_hyperlink": false
            }
          },
          {
            "type": "git",
            "style": "plain",
            "foreground": "#95ffa4",
            "properties": {
              "branch_ahead_icon": "\u2191",
              "branch_behind_icon": "\u2193",
              "branch_gone": "\u2262",
              "branch_icon": "\ue0a0 ",
              "branch_identical_icon": "\u2261",
              "cherry_pick_icon": "\u2713 ",
              "color_background": false,
              "commit_icon": "\u25b7 ",
              "display_status_detail": true,
              "local_staged_icon": "",
              "local_working_icon": "",
              "merge_icon": "\u25f4 ",
              "no_commits_icon": "[no commits]",
              "rebase_icon": "\u2c62 ",
              "stash_count_icon": "",
              "status_colors_enabled": true,
              "status_separator_icon": " \u2502",
              "tag_icon": "\u25b6 ",
              "local_changes_color": "#ff9248",
              "ahead_and_behind_color": "#f26d50",
              "behind_color": "#f17c37",
              "ahead_color": "#95ffa4"
            }
          },
          {
            "type": "dotnet",
            "style": "plain",
            "foreground": "#178600",
            "properties": {
              "prefix": " \uE77F "
            }
          },
          {
            "type": "java",
            "style": "plain",
            "foreground": "#B07219",
            "properties": {
              "prefix": " \uE738 "
            }
          },
          {
            "type": "node",
            "style": "plain",
            "foreground": "#73AA63",
            "properties": {
              "prefix": " \uE718 "
            }
          },
          {
            "type": "ruby",
            "style": "plain",
            "foreground": "#CD2A18",
            "properties": {
              "prefix": " \uE791 "
            }
          },
          {
            "type": "exit",
            "style": "plain",
            "foreground": "#ff8080",
            "properties": {
              "display_exit_code": false,
              "prefix": " ",
              "postfix": " ",
              "error_icon": "error"
            }
          }
        ]
      },
      {
        "type": "prompt",
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "type": "text",
            "style": "plain",
            "foreground": "#91ddff",
            "properties": {
              "prefix": "",
              "postfix": "",
              "text": "\u2514 "
            }
          },
          {
            "type": "text",
            "style": "plain",
            "foreground": "#ffffff",
            "properties": {
              "prefix": "",
              "postfix": "",
              "text": "$"
            }
          }
        ]
      }
    ],
    "final_space": true
  }
  